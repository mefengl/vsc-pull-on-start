{
  "publisher": "mefengl",
  "name": "pull-on-start",
  "displayName": "Pull on Start",
  "version": "0.0.1",
  "private": true,
  "description": "Run git pull when start vscode",
  "author": "mefengl",
  "license": "MIT",
  "repository": "https://github.com/mefengl/vsc-pull-on-start",
  "keywords": [
    "vscode",
    "extension",
    "git",
    "pull",
    "startup",
    "automation",
    "repository"
  ],
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "main": "./extension.js",
  "icon": "res/icon.png",
  "engines": {
    "vscode": "^1.84.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Pull on Start",
      "properties": {
        "pullOnStart.silence": {
          "type": "boolean",
          "default": true,
          "description": "Silence informational messages (errors will still be shown)."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.84.0",
    "eslint": "^8.53.0"
  }
}
